<p id="notice"><%= notice %></p>

<h1>Type Accounts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_accounts.each do |type_account| %>
      <tr>
        <td><%= type_account.name %></td>
        <td><%= link_to 'Show', type_account %></td>
        <td><%= link_to 'Edit', edit_type_account_path(type_account) %></td>
        <td><%= link_to 'Destroy', type_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Account', new_type_account_path %>
